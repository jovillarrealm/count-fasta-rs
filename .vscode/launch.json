    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "no csv Debug executable 'count-fasta-rs'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=count-fasta-rs",
                    "--package=count-fasta-rs"
                ],
                "filter": {
                    "name": "count-fasta-rs",
                    "kind": "bin"
                }
            },
            "args": ["../count-fasta-c/GENOMIC/GCA_000009765_Streptomyces-avermitilis_MA-4680.fna"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "csv Debug executable 'count-fasta-rs'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=count-fasta-rs",
                    "--package=count-fasta-rs"
                ],
                "filter": {
                    "name": "count-fasta-rs",
                    "kind": "bin"
                }
            },
            "args": ["-c", "data.csv","../count-fasta-c/GENOMIC/GCA_000009765_Streptomyces-avermitilis_MA-4680.fna"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'count-fasta-rs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=count-fasta-rs",
                    "--package=count-fasta-rs"
                ],
                "filter": {
                    "name": "count-fasta-rs",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}